{
  "name": "kartel-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest watch --globals",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview :5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5173' :5173 'cypress open --e2e'",
    "test:component": "cypress run --component",
    "test:component:dev": "cypress open --component",
    "//comment-test:dev": "The below scripts are intended to be run against the dev server running npm run dev in a separate terminal window",
    "test:e2e:dev:coverage": "cypress run --e2e",
    "test:component:dev:coverage": "cypress run --component --quiet --reporter spec",
    "test:dev:coverage": "npm run test:component:dev:coverage & npm run test:e2e:dev:coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.17",
    "axios": "^1.3.5",
    "marked": "^4.3.0",
    "pinia": "^3.0.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/vue": "^5.0.4",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tailwindcss/postcss": "^4.1.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/coverage-istanbul": "^0.29.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^14.2.1",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^21.1.0",
    "mermaid": "^11.6.0",
    "postcss": "^8.4.21",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^4.1.3",
    "vite": "^6.2.5",
    "vite-plugin-istanbul": "^4.0.1",
    "vitepress": "^1.0.0-alpha.43",
    "vitepress-plugin-mermaid": "^2.0.10",
    "vitest": "^0.29.2"
  }
}
