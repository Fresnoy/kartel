{
  "name": "kartel-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest run --printConsoleTrace=true --silent=false",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview :5173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5173' :5173 'cypress open --e2e'",
    "test:component": "cypress run --component",
    "test:component:dev": "cypress open --component",
    "//comment-test:dev": "The below scripts are intended to be run against the dev server running npm run dev in a separate terminal window",
    "test:e2e:dev:coverage": "cypress run --e2e",
    "test:component:dev:coverage": "cypress run --component --quiet --reporter spec",
    "test:dev:coverage": "npm run test:component:dev:coverage & npm run test:e2e:dev:coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.36",
    "axios": "^1.12.2",
    "marked": "^15.0.12",
    "pinia": "^3.0.3",
    "vue": "^3.2.45",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@cypress/vue": "^6.0.2",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.36.0",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^26.1.0",
    "mermaid": "^11.12.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.11",
    "vite": "^6.3.6",
    "vite-plugin-istanbul": "^7.2.0",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^3.1.1"
  }
}
